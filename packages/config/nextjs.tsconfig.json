{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "NextJS",
	"extends": "./base.tsconfig.json",
	"compilerOptions": {
		"target": "ES5",
		"allowJs": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"incremental": true
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
